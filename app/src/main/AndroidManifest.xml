<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.Login.UserLogin"
    android:versionCode="2"
    android:versionName="1.1" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="18" />

    <application
        android:allowBackup="true"
        android:name="com.wanfang.application.myApplication"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:theme="@style/yzTest">
        <activity
            android:name="com.wanfang.Activity.LoginActivity"
            android:theme="@android:style/Theme.NoTitleBar.Fullscreen"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.wanfang.Activity.MainActivity" android:theme="@android:style/Theme.NoTitleBar.Fullscreen"></activity>
        <activity android:name="com.wanfang.Activity.ReviewActivity"></activity>
        <activity android:name="com.wanfang.Activity.OEMInfoActivity" android:theme="@android:style/Theme.Dialog"></activity>
        <activity android:name="com.wanfang.Activity.RefuseActivity" android:theme="@android:style/Theme.Dialog"></activity>
        <activity android:name="com.wanfang.Activity.PassReviewActivity" android:theme="@android:style/Theme.Dialog"></activity>
    </application>
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/>
</manifest>
